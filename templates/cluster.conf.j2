[ req ]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[ dn ]
CN = clickhouse.your-company.com

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = clickhouse.your-company.com
{% for id, dns_name in private_dns | dictsort %}
DNS.{{ id + 2 }} = {{ dns_name }}
{% endfor %}
